class Solution {
    int n;
    int m;
    Integer[][][] dp;
    public int solve(int[][] grid, int r, int c1, int c2){
        if(r>=n){
            return 0;
        }
        if(dp[r][c1][c2]!=null){
            return dp[r][c1][c2];
        }
        int cherry = grid[r][c1];
        if(c1!=c2){
            cherry+=grid[r][c2];
        }
        int ans = 0;
        for(int i=-1;i<=1;i++){
            for(int j=-1;j<=1;j++){
                int nr = r+1;
                int nc1 = c1+i;
                int nc2 = c2+j;

                if(nc1>=0&&nc1<m&&nc2>=0&&nc2<m){
                    ans = Math.max(ans,solve(grid,nr,nc1,nc2));
                }
            }
        }
        return dp[r][c1][c2] = cherry + ans;
    }
    public int cherryPickup(int[][] grid) {
        n = grid.length;
        m = grid[0].length;
        dp = new Integer[n][m][m];
        return solve(grid,0,0,m-1);
    }
}
