class Solution {
    public int solve(int[] nums, int idx, int end, Integer[] dp){
        if(idx>end){
            return 0;
        }
        if(dp[idx]!=null){
            return dp[idx];
        }
        int take = nums[idx] + solve(nums,idx+2,end,dp);
        int nottake = solve(nums,idx+1,end,dp);
        return dp[idx] = Math.max(take,nottake);
    }
    public int rob(int[] nums) {
        int n = nums.length;
        if(n==1){
            return nums[0];
        }
        Integer[] dp1 = new Integer[nums.length];
        Integer[] dp2 = new Integer[nums.length];
        int case1 = solve(nums,0,n-2,dp1);
        int case2 = solve(nums,1,n-1,dp2);
        return Math.max(case1,case2);
    }
}
