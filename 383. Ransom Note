class Solution {
    public boolean canConstruct(String r, String m) {
        char[] rn = r.toCharArray();
        char[] mz = m.toCharArray();
        Arrays.sort(rn);
        Arrays.sort(mz);
        int i=0;
        int j=0;
        while(i<r.length() && j<m.length()){
            if(rn[i]==mz[j]){
                i++;
                j++;
            }else if(rn[i]<mz[j]){
                return false;
            }else{
                j++;
            }
            
        }
        return i==r.length();
    }
}
