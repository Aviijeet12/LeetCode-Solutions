/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public void solve(TreeNode root, int tg, int sum, List<List<Integer>> res, ArrayList<Integer> curr){
        if(root==null){
            return;
        }
        sum += root.val;
        curr.add(root.val);
        if(root.left==null&&root.right==null){
            if(tg==sum){
                res.add(new ArrayList<>(curr));
            }
        }else{
            solve(root.left,tg,sum,res,curr);
            solve(root.right,tg,sum,res,curr);
        }
        curr.remove(curr.size()-1);
    }
    public List<List<Integer>> pathSum(TreeNode root, int tg) {
        List<List<Integer>> res = new ArrayList<>();
        solve(root,tg,0,res,new ArrayList<>());
        return res;
    }
}
