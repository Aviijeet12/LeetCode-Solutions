class Solution {
    public int repeatedStringMatch(String a, String b) {
        String s = "";
        int cnt = 0;
        
        while(s.length()<b.length()){
            s+=a;
            cnt++;
        }
        if(s.indexOf(b)!=-1){
            return cnt;
        }
        s+=a;
        cnt++;
        if(s.indexOf(b)!=-1){
            return cnt;
        }
        return -1;
    }
}
