class Solution {
    public List<Boolean> kidsWithCandies(int[] candies, int extraCandies) {
        int max = 0;
        for(int i : candies){
            max = Math.max(i,max);
        }
        List<Boolean> res = new ArrayList<>();
        boolean[] ans = new boolean[candies.length];
        for(int i=0;i<candies.length;i++){
            int temp = candies[i] + extraCandies;
            if(temp>=max){
                ans[i] = true;
            }else{
                ans[i] = false;
            }
        }
        for(boolean i : ans){
            res.add(i);
        }
        return res;
    }
}
