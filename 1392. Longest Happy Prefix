class Solution {
    public void computelps(int[] lps, String s){
        lps[0] = 0;
        int len = 0;
        int i=1;
        while(i<s.length()){
            if(s.charAt(i)==s.charAt(len)){
                len++;
                lps[i] = len;
                i++;
            }else{
                if(len!=0){
                    len = lps[len-1];
                }else{
                    lps[i]=0;
                    i++;
                }
            }
        }
    }
    public String longestPrefix(String s) {
        int n = s.length();
        int[] lps = new int[n];
        computelps(lps,s);
        // int idx = 0;
        // for(int i=0;i<n;i++){
        //     if(lps[i]>lps[idx]){
        //         idx = i;
        //     }
        // }
        // return s.substring(idx-lps[idx]+1,idx+1);
        int len = lps[n-1];   
        return s.substring(0, len);
    }
}
