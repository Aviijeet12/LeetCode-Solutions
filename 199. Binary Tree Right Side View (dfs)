/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public void compute(TreeNode root, int lvl, List<Integer> a){
        if(root==null){
            return;
        }
        if(a.size()<lvl){
            a.add(root.val);
        }
        compute(root.right,lvl+1,a);
        compute(root.left,lvl+1,a);
    }
    public List<Integer> rightSideView(TreeNode root) {
        List<Integer> a = new ArrayList<>();
        if(root==null){
            return a;
        }
        compute(root,1,a);
        return a;
    }
}
